#: samba/filepropertiesplugin/sambausershareplugin.cpp:247
#, kde-format
msgctxt "@info detailed error messsage"
msgid ""
"You have exhausted the maximum amount of shared directories you may have "
"active at the same time."
msgstr "您已经达到了同时活跃的共享目录的最大数量限制。"

#: samba/filepropertiesplugin/sambausershareplugin.cpp:249
#, kde-format
msgctxt "@info detailed error messsage"
msgid "The share name is invalid."
msgstr "共享名称无效。"

#: samba/filepropertiesplugin/sambausershareplugin.cpp:251
#, kde-format
msgctxt "@info detailed error messsage"
msgid "The share name is already in use for a different directory."
msgstr "此共享名称已被用于另一个目录。"

#: samba/filepropertiesplugin/sambausershareplugin.cpp:253
#, kde-format
msgctxt "@info detailed error messsage"
msgid "The path is invalid."
msgstr "路径无效。"

#: samba/filepropertiesplugin/sambausershareplugin.cpp:255
#, kde-format
msgctxt "@info detailed error messsage"
msgid "The path does not exist."
msgstr "此路径不存在。"

#: samba/filepropertiesplugin/sambausershareplugin.cpp:257
#, kde-format
msgctxt "@info detailed error messsage"
msgid "The path is not a directory."
msgstr "此路径不是一个目录。"

#: samba/filepropertiesplugin/sambausershareplugin.cpp:259
#, kde-format
msgctxt "@info detailed error messsage"
msgid "The path is relative."
msgstr "此路径是相对路径。"

