#, kde-format
msgctxt "@info detailed error messsage"
msgid "This path may not be shared."
msgstr "此路径不能共享。"

#: samba/filepropertiesplugin/sambausershareplugin.cpp:263
#, kde-format
msgctxt "@info detailed error messsage"
msgid "The access rule is invalid."
msgstr "此访问规则无效。"

